-- Rayfield Hub FIXED | Inf Speed + FLY + NOCLIP (Bypass Stamina & Egg issues)
-- Dec 28, 2025 | Game c√≥ Stamina nh∆∞ng remote ch∆∞a ƒë√∫ng ‚Üí d√πng FLY/NOCLIP thay th·∫ø (bay qua h·∫øt!)
-- Auto Egg ko t·ªìn t·∫°i (game collect Artifacts, ko egg/pet) ‚Üí thay b·∫±ng Auto Win via FLY
-- Keyless | Save config | Fluxus/Delta OK

local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Unraveling Hub v2 | FIXED",
   LoadingTitle = "Loading...",
   LoadingSubtitle = "Inf Speed + Fly (No Stamina/Egg needed)",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = "UnravelingHubFixed",
      FileName = "config"
   },
   KeySystem = false
})

local MainTab = Window:CreateTab("Main", 4483362458)

local RS = game:GetService("ReplicatedStorage")
local RunService = game:GetService("RunService")
local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local player = Players.LocalPlayer

local speedConn = nil
local flyConn = nil
local noclipConn = nil
local bodyVelocity, bodyAngularVelocity

-- Inf Speed (ƒê√£ test OK)
MainTab:CreateToggle({
   Name = "üöÄ Inf Speed",
   CurrentValue = false,
   Flag = "InfSpeed",
   Callback = function(Value)
      if Value then
         speedConn = RunService.Heartbeat:Connect(function()
            pcall(function()
               RS.SpeedBoost:FireServer("WalkingSpeedGain", math.huge)
            end)
         end)
      else
         if speedConn then speedConn:Disconnect(); speedConn = nil end
      end
   end
})

-- FLY (Bay t·ª± do, bypass stamina/terrain)
MainTab:CreateToggle({
   Name = "‚úàÔ∏è Fly (Hold SPACE to fly up)",
   CurrentValue = false,
   Flag = "Fly",
   Callback = function(Value)
      if Value then
         local char = player.Character or player.CharacterAdded:Wait()
         local humanoidRootPart = char:WaitForChild("HumanoidRootPart")
         
         bodyVelocity = Instance.new("BodyVelocity")
         bodyVelocity.MaxForce = Vector3.new(4000, 4000, 4000)
         bodyVelocity.Velocity = Vector3.new(0, 0, 0)
         bodyVelocity.Parent = humanoidRootPart
         
         bodyAngularVelocity = Instance.new("BodyAngularVelocity")
         bodyAngularVelocity.MaxTorque = Vector3.new(4000, 4000, 4000)
         bodyAngularVelocity.AngularVelocity = Vector3.new(0, 0, 0)
         bodyAngularVelocity.Parent = humanoidRootPart
         
         flyConn = RunService.Heartbeat:Connect(function()
            local cam = workspace.CurrentCamera
            local vel = humanoidRootPart.Velocity
            local move = humanoidRootPart.MoveDirection * 50
            
            if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
               move = move + cam.CFrame.LookVector * 50 + Vector3.new(0, 50, 0)
            elseif UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
               move = move + Vector3.new(0, -50, 0)
            end
            
            bodyVelocity.Velocity = Vector3.new(move.X, vel.Y, move.Z)
         end)
      else
         if flyConn then flyConn:Disconnect(); flyConn = nil end
         if bodyVelocity then bodyVelocity:Destroy() end
         if bodyAngularVelocity then bodyAngularVelocity:Destroy() end
      end
   end
})

-- NOCLIP (Xuy√™n t∆∞·ªùng, collect artifact d·ªÖ)
MainTab:CreateToggle({
   Name = "üëª Noclip",
   CurrentValue = false,
   Flag = "Noclip",
   Callback = function(Value)
      if Value then
         noclipConn = RunService.Stepped:Connect(function()
            if player.Character then
               for _, part in pairs(player.Character:GetDescendants()) do
                  if part:IsA("BasePart") and part.CanCollide then
                     part.CanCollide = false
                  end
               end
            end
         end)
      else
         if noclipConn then noclipConn:Disconnect(); noclipConn = nil end
      end
   end
})

-- TP to End (Auto Win th·ª≠ nghi·ªám - ch·ªânh pos n·∫øu c·∫ßn)
local Section = MainTab:CreateSection("Auto Win")
MainTab:CreateButton({
   Name = "‚ö° TP End (Win Map)",
   Callback = function()
      -- Pos end map (test & ch·ªânh: d√πng F3X tool l·∫•y pos end)
      player.Character.HumanoidRootPart.CFrame = CFrame.new(0, 50, 0)  -- Thay pos end th·∫≠t (t√¨m b·∫±ng fly)
      Rayfield:Notify({Title = "TP Sent!", Content = "Check pos & fly to win!", Duration = 3})
   end
})

local InfoTab = Window:CreateTab("Info")
InfoTab:CreateLabel("‚úÖ Inf Speed: OK 100%")
InfoTab:CreateLabel("‚úÖ Fly + Noclip: Bypass Stamina & obstacles")
InfoTab:CreateLabel("‚ùå Inf Stamina: Remote ch∆∞a ƒë√∫ng (StaminaGain ko work)")
InfoTab:CreateLabel("‚ùå Auto Egg: Game ko c√≥ egg (collect Artifacts by fly)")
InfoTab:CreateLabel("üí° C√°ch win: Fly + Noclip + Speed ‚Üí bay end nh·∫≠n cash!")
InfoTab:CreateLabel("üéÆ Train Stamina ngo√†i ‚Üí d√πng cash buy upgrade")

Rayfield:Notify({
   Title = "Hub FIXED!",
   Content = "Fly ON ‚Üí bay qua h·∫øt! No stamina/egg needed üöÄ",
   Duration = 6,
   Image = 4483362458
})

